import type { Command } from '@stratix/abstractions';

export interface Create<%- entityName %>Input {
<% props.forEach(prop => { -%>
  <%- prop.name %>: <%- prop.type %>;
<% }); -%>
}

export interface Create<%- entityName %>Output {
  <%- entityNameCamel %>Id: string;
}

/**
 * Command to create a new <%- entityName %>.
 */
export class Create<%- entityName %>Command implements Command {
  constructor(public readonly data: Create<%- entityName %>Input) {}
}
