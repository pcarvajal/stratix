# OpenAI Configuration (for Level 3+)
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Anthropic Configuration (alternative to OpenAI)
# Get your API key from: https://console.anthropic.com
ANTHROPIC_API_KEY=

# Agent Configuration
AGENT_LOG_LEVEL=info
AGENT_MAX_RETRIES=3
AGENT_TIMEOUT_MS=30000

# Budget Configuration (optional)
# Set max cost per execution (in USD)
AGENT_MAX_COST_PER_EXECUTION=1.0

# Set max cost per session
AGENT_MAX_COST_PER_SESSION=10.0

# Environment
NODE_ENV=development
