{
  "name": "my-stratix-app",
  "version": "0.1.0",
  "description": "Stratix AI Agents Learning Path",
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "example:echo": "tsx src/agents/01-echo/index.ts",
    "example:mock": "tsx src/agents/02-mock/index.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\""
