  "dependencies": {
    "@stratix/primitives": "^0.1.0",
    "@stratix/abstractions": "^0.1.0",
    "@stratix/runtime": "^0.1.0",
    "@stratix/impl-ai-agents": "^0.1.0",
    "@stratix/ext-ai-agents-openai": "^0.1.0",
    "@stratix/ext-ai-agents-anthropic": "^0.1.0",
    "inquirer": "^9.2.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/inquirer": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
